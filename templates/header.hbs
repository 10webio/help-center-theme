<div class="top-bar-container hidden">
    <div class="container">
        <div class="top-bar-logo"></div>
        <div class="top-bar__content">
            <p class="top"><b>10Web AI Assistant</b> is live on PRODUCT HUNT</p>
        </div>
        <div class="top-bar__link">
            <a href="https://www.producthunt.com/posts/10web-ai-assistant-for-wordpress" target="_blank"  class="tw_button ">Show
                Support</a>
        </div>
        <div class="top-bar-container__close"></div>
    </div>
</div>

<header class="header">
  <div class="header_fixed">
     <div class="header_container container">
        <div class="header_left">
          <div class="menu-icon">
            <div>
              <span></span>
              <span></span>
            </div>
          </div>
          <div class="logo">
            {{#link 'help_center'}}
              <div class="help-center-logo" ></div>
            {{/link}}
          </div>
          {{#link 'requests' class='my-tickets-link logged-in'}}My Tickets{{/link}}
      </div>
      <div class="header_right">
        <div class="header_search">
          <span class="toggle-search icon-search"></span>
          {{search submit=false instant=settings.instant_search class='search' placeholder='Search The Help Center'}}
          <div class="icon-close"></div>
        </div>
        <div class="user_info_section">
          {{user_info}}
        </div>
      </div>
   </div>
   <div id="mobile-menu">
     <div class="mobile-menu-logged-in">
       <div class="container">
        {{#link 'requests' class='my-tickets-link logged-in'}}My Tickets{{/link}}
       </div>
     </div>
     <div class="container">
       <div class="mobile-menu-container"></div>
       <div class="mobile-menu-logged-out"></div>
     </div>
    </div>
  </div>
</header>
