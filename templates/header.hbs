<!--Black Friday offer-->
<div class="top-bar-container hidden">
    <div class="container">
        <div class="top-bar-container__content">
            <div>20% OFF ON <br class="mobile">ALL 10WEB PLANS !!!</div>
        </div>
        <div class="top-bar-container__coupon">
            <div class="coupon_info">
                <span>Coupon code:</span>
                <span class="coupon">BLK20</span>
                <input type="hidden" value="BLK20" id="coupon_code">
            </div>
            <div class="copy_container">
                <span>Copy</span>
            </div>
        </div>
    </div>
    <div class="top-bar-container__close"></div>
</div>
<!--Black Friday offer end-->

<header class="header">
  <div class="header_fixed">
     <div class="header_container container">
        <div class="header_left">
          <div class="menu-icon">
            <div>
              <span></span>
              <span></span>
            </div>
          </div>
          <div class="logo">
            {{#link 'help_center'}}
              <div class="help-center-logo" ></div>
            {{/link}}
          </div>
          {{#link 'requests' class='my-tickets-link logged-in'}}My Tickets{{/link}}
      </div>
      <div class="header_right">
        <div class="header_search">
          <span class="toggle-search icon-search"></span>
          {{search submit=false instant=settings.instant_search class='search' placeholder='Search The Help Center'}}
          <div class="icon-close"></div>
        </div>
        <div class="user_info_section">
          {{user_info}}
        </div>
      </div>
   </div>
   <div id="mobile-menu">
     <div class="mobile-menu-logged-in">
       <div class="container">
        {{#link 'requests' class='my-tickets-link logged-in'}}My Tickets{{/link}}
       </div>
     </div>
     <div class="container">
       <div class="mobile-menu-container"></div>
       <div class="mobile-menu-logged-out"></div>
     </div>
    </div>
  </div>
</header>
